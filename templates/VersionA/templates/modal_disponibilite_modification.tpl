                    dataType: 'html',
                    success: function (data) {
                        $('#idbox').html("<option value=''></option>" + data);
                    }
                });

                $.ajax({
                    url: '../ajax/liste_uniter_fonctionnelle_praticien.php',
                    data: {
                        'alaxione_lieu_user': $(this).val(),
                        'id_praticien': $('#id_praticien').val(),
                        'id_planning': {/literal}{$idtranche}{literal},
                    },
                    dataType: 'html',
                    success: function (data) {
                        $('#idservice').html("<option value=''></option>" + data);
                        $('#divunitefonctionnelle').css('display', 'block');
                        $('#idservice').attr("required", "true");
                    },
                    error: function () {
                        $('#idservice').html("<option value=''></option>");
                        $('#divunitefonctionnelle').css('display', 'none');
                        $('#idservice').removeAttr("required");
                    }
                });
            });
            if ($('#idservice').val() === "") {
                $("#idlieu").trigger('change');
            }
        });

